{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/register/page.js", "src/app/login/page.js", "src/clientComponents/ChannelContainer.jsx", "src/app/profile/page.js", "src/app/api/users/updateUser/route.js", "src/components/DietPlanner.jsx", "src/components/Feedback.jsx", "src/components/HealthRecords.jsx", "src/components/Navbar.jsx", "src/components/NGOFinder.jsx"],
  "exclude": ["node_modules"]
}
